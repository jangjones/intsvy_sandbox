# Download the source code .tar.gz for the 'intsvy' package into the working directory
download.packages(pkgs = "intsvy", destdir = ".", type = "source")  

# Unpack this
untar("intsvy_2.1.tar.gz", list=TRUE, files = "/intsvy.csv")

